<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kishor.farmersbid">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:name="android.hardware.wifi" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.LoginActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.FarmerAddProduct"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.SeeBid"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.Farmer_History_Product"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.Register_cust"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.Register_Farmer"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.BidList"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.Consumer_Bidding_Product"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.consumer_history"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".activities.Consumer_win_list"
            android:screenOrientation="portrait"></activity>

    </application>

</manifest>